{
  "name": "metaweb-cli",
  "version": "0.0.1",
  "description": "Command line tool for metawebjs",
  "main": "dist/cli.js",
  "bin": "dist/cli.js",
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build",
    "test": "mocha --require ts-node/register src/**/*.test.ts --reporter list",
    "test-watch": "mocha --watch src/ --watch-extensions tsx,ts --require ts-node/register --recursive './src/**/*.test.ts'  --reporter list"
  },
  "keywords": [
    "metaweb",
    "js",
    "cli"
  ],
  "author": "Eric Wooley",
  "license": "MIT",
  "dependencies": {
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "dustjs-linkedin": "^2.7.4",
    "lerna": "^2.0.0-beta.30",
    "lodash": "^4.17.2",
    "promise": "^7.1.1",
    "recursive-readdir": "^2.1.0",
    "shelljs": "^0.7.5"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "mocha": "^3.1.2",
    "sinon": "^1.17.6",
    "ts-node": "^1.7.0",
    "typescript": "^2.0.10"
  }
}
