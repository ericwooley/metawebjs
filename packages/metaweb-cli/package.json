{
  "name": "metaweb-cli",
  "version": "0.0.1",
  "description": "Command line tool for metawebjs",
  "main": "dist/index.js",
  "bin": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "prepublish": "npm run build",
