    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "lerna": "^1.1.3",
    "lodash": "^4.17.2",
    "mocha": "^3.1.2",
    "shelljs": "^0.7.5",
    "ts-node": "^1.7.0",
    "typescript": "^2.0.10"
  }
}
